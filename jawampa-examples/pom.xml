<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.github.sshaddicts.jawampa</groupId>
        <version>0.0.1</version>
        <artifactId>jawampa-parent</artifactId>
    </parent>

    <artifactId>jawampa-examples</artifactId>
    <packaging>jar</packaging>

    <name>jawampa-examples</name>
    <url></url>
    <description>
        Examples for the usage of jawampa
    </description>

    <licenses>
        <license>
            <name>Apache License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0</url>
        </license>
    </licenses>

    <inceptionYear>2014</inceptionYear>

    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>jawampa-netty</artifactId>
            <version>${project.version}</version>
        </dependency>
    </dependencies>
</project>
